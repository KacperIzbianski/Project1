<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/viewNumber3"
        android:layout_width="46"
        android:layout_height="39"
        android:text="@string/viewNumber3"
        app:layout_constraintEnd_toStartOf="@+id/viewNumber4"
        tools:layout_editor_absoluteY="64dp" />

    <TextView
        android:id="@+id/viewNumber1"
        android:layout_width="46dp"
        android:layout_height="39dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="64dp"
        android:text="@string/viewNumber1"
        app:layout_constraintEnd_toStartOf="@+id/viewNumber2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/viewNumber2"
        android:layout_width="46"
        android:layout_height="39"
        android:text="@string/viewNumber2"
        app:layout_constraintEnd_toStartOf="@+id/viewNumber3"
        tools:layout_editor_absoluteY="64dp" />

    <TextView
        android:id="@+id/viewNumber4"
        android:layout_width="46"
        android:layout_height="39"
        android:text="@string/viewNumber4"
        app:layout_constraintEnd_toStartOf="@+id/viewNumber5"
        tools:layout_editor_absoluteY="64dp" />

    <TextView
        android:id="@+id/viewNumber5"
        android:layout_width="46"
        android:layout_height="39"
        android:layout_marginEnd="72dp"
        android:text="@string/viewNumber5"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="64dp" />

</android.support.constraint.ConstraintLayout>